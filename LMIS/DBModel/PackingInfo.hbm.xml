<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="LMIS.DBModel"
                   assembly="LMIS">

  <!-- Mappings for class 'Customer' -->
  <class name="PackingInfo" table="PackingInfo" lazy="false">

    <!-- Identity mapping -->
    <id name="PackingInfoId">
      <column name="PackingInfoId" />
      <generator class="native" />
    </id>

    <!-- Simple mappings -->

    <property name="PackingName"/>
    <property name="PackingDescription"/>

  </class>

</hibernate-mapping>