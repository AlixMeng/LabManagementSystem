    <!-- Identity mapping -->
    <id name="Id">
      <column name="Id" />
      <generator class="native" />
    </id>

    <!-- Simple mappings -->

   
    <property name="JobBottleDetailId"/>
    <property name="NumberOfBottleCollect"/>
    <property name="SampleCollectDate"/>
    <property name="EventId"/>
    <property name="TestId"/>
    <property name="AssigneDate"/>
    <property name="TracingPoint"/>
    <property name="IsSampleTaken"/>
    <property name="SampleInformation"/>
    <property name="SampleDescription"/>
    <property name="TypeOfSampling"/>
    <property name="PackingId"/>
    <property name="BottleList"/>
    <property name="JobDetailID"/>

    
  </class>

</hibernate-mapping>