<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="LMIS.DBModel"
                   assembly="LMIS">

  <class name="JobTestBottleDetail" table="JobTestBottleDetail" lazy="false">

    <!-- Identity mapping -->
    <id name="ID">
      <column name="ID" />
      <generator class="native" />
    </id>

    <!-- Simple mappings -->
    <property name="JobDetailId"/>
    <property name="BottleId"/>
    <property name="BottleRequired"/>
    <property name="TestId"/>
    <property name="BottleType"/>
    <property name="JobDetailParentId"/>
    <property name="JobInfoId"/>
    
    
  </class>

</hibernate-mapping>