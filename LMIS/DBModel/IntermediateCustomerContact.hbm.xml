<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="LMIS.DBModel"
                   assembly="LMIS">

  <!-- Mappings for class 'Contact' -->
  <class name="IntermediateCustomerContact" table="IntermediateCustomerContact" lazy="false">
    <!-- Identity mapping -->
    <id name="Id">
      <column name="Id" />
      <generator class="native" />
    </id>
    <!-- Simple mappings -->
    <property name="CustID"/>
    <property name="ContactID"/>
    <property name="JobID"/>
  </class>

</hibernate-mapping>