<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="LMIS.DBModel"
                   assembly="LMIS">

  <class name="Appointment" table="Appointment" lazy="false">

    <!-- Identity mapping -->
    <id name="CalendarKey">
      <column name="CalendarKey" />
      <generator class="native" />
    </id>

    <!-- Simple mappings -->
    <property name="Title"/>
    <property name="StartDate"/>
    <property name="EndDate"/>
    <property name="AllDayEvent"/>
    <property name="BackgroundColor"/>
    <property name="AppointmentType"/>
    
    
  </class>

</hibernate-mapping>